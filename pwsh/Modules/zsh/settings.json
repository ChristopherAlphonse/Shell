{
    "editor.multiCursorModifier": "ctrlCmd",
    "diffEditor.wordWrap": "on",

    "security.workspace.trust.untrustedFiles": "open",
    "editor.linkedEditing": true,
    "files.autoSaveDelay": 1,
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "js/ts.implicitProjectConfig.strictNullChecks": true,
    "search.useIgnoreFiles": false,

    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },
    "security.workspace.trust.banner": "always",
    "html.autoClosingTags": false,
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "explorer.confirmDragAndDrop": false,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "files.associations": {
        "*.vim": "vimrc",
        "*.json": "json",
        "*.js": "javascript",
        "*.jsx": "javascriptreact"
    },
    "editor.fontFamily": "FiraCode",
    "todoHightLight.isEnable": "true",
    "editor.fontLigatures": false,
    "settingsSync.ignoredSettings": [],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "editor.suggest.insertMode": "replace",

    "editor.tabSize": 4,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "editor.letterSpacing": 1,
    "editor.renderWhitespace": "all",
    "editor.cursorStyle": "line-thin",
    "editor.matchBrackets": "always",
    "[css]": {
        "editor.suggest.insertMode": "insert",
        "editor.lineHeight": 25,
        "editor.tabSize": 4,
        "editor.wordWrap": "on",
        "editor.wordWrapColumn": 80,
        "editor.letterSpacing": 1.2,
        "editor.renderWhitespace": "selection",
        "editor.occurrencesHighlight": "singleFile",
        "editor.cursorStyle": "line-thin",
        "editor.matchBrackets": "always"
    },

    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "js/ts.implicitProjectConfig.strictFunctionTypes": false,
    "debug.terminal.clearBeforeReusing": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.fontWeight": "300",
    "terminal.integrated.gpuAcceleration": "off",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.splitCwd": "workspaceRoot",
    "typescript.updateImportsOnFileMove.enabled": "always",

    "[javascriptreact]": {},
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "git.ignoreRebaseWarning": true,
    "terminal.integrated.env.windows": {},
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        }
    },

    "emmet.includeLanguages": {
        "postcss": "css"
    },
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.addMissingImports": "explicit",
        "source.fixAll.eslint": "explicit"
    },
    "terminal.integrated.smoothScrolling": true,
    "markdown.preview.lineHeight": 2,
    "terminal.integrated.cursorWidth": 0,
    "terminal.integrated.fontSize": 10,
    "editor.guides.indentation": true,
    "editor.guides.bracketPairs": true,
    "workbench.editorAssociations": {
        "*.dll": "default",
        "*.vsix": "default"
    },
    "json.schemas": [],
    "redhat.telemetry.enabled": true,
    "files.autoSave": "onFocusChange",
    "editor.formatOnPaste": true,

    "workbench.statusBar.visible": true,
    "files.eol": "\n",
    "javascript.suggest.completeFunctionCalls": true,
    "cSpell.languageSettings": [],
    "security.workspace.trust.enabled": false,
    "editor.accessibilitySupport": "off",
    "liveServer.settings.ChromeDebuggingAttachment": true,

    "lldb.suppressUpdateNotifications": true,

    "typescript.referencesCodeLens.enabled": true,

    "eslint.enable": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],

    "typescript.suggest.completeFunctionCalls": true,
    "typescript.validate.enable": true,

    "typescript.preferences.importModuleSpecifier": "relative",

    "eslint.format.enable": true,
    "scssFormatter.singleQuote": true,
    "docker.languageserver.formatter.ignoreMultilineInstructions": true,
    "notebook.formatOnCellExecution": true,
    "notebook.formatOnSave.enabled": true,
    "java.errors.incompleteClasspath.severity": "ignore",

    "java.signatureHelp.enabled": true,

    "java.test.config": {
        "displayTestLinks": true,
        "workingDirectory": "${workspaceFolder}"
    },
    "java.debug.settings.vmArgs": "-Dfile.encoding=UTF-8",

    "java.project.referencedLibraries": ["lib/**/*.jar"],
    "java.completion.importOrder": [
        "",
        "java",
        "javax",
        "org",
        "com",
        "io",
        "net"
    ],

    "cSpell.userWords": [
        "autofetch",
        "Awsorganizations",
        "Azalais",
        "CAPEX",
        "Caskaydia",
        "Classpath",
        "Dfile",
        "donot",
        "dont",
        "esbenp",
        "Fira",
        "javax",
        "mwpb",
        "openapi",
        "OPEX",
        "pwsh",
        "swaggerui",
        "Sysnative",
        "vimrc",
        "vsicons",
        "vsix",
        "windir"
    ],
    "editor.formatOnSave": true,
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Azalais",

    "editor.tokenColorCustomizations": {
        "[Azalais Glow]": {
            "textMateRules": [
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#40a6fa84"
                    }
                },
                {
                    "scope": "variable.other.constant",
                    "settings": {
                        "foreground": "#fae44094"
                    }
                }
            ]
        },
        "[Azalais]": {
            "textMateRules": [
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#40A6FA"
                    }
                },
                {
                    "scope": "variable.other.constant",
                    "settings": {
                        "foreground": "#DCC61C"
                    }
                }
            ]
        }
    },
    "sonarlint.focusOnNewCode": false,
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    },

    "javascript.validate.enable": true,

    "terminal.integrated.fontFamily": "FiraCode Nerd Font,FiraMono Nerd Font, MesloGLDZ Nerd Font, Monaco, 'Courier New', monospace",

    "reactSnippets.settings.prettierEnabled": true,
    "prettier.trailingComma": "all",
    "prettier.tabWidth": 4,

    "sonarlint.rules": {
        "javascript:S6774": {
            "level": "off"
        },
        "java:S106": {
            "level": "off"
        },
        "java:S120": {
            "level": "off"
        },
        "java:S101": {
            "level": "off"
        },
        "java:S1068": {
            "level": "off"
        },
        "java:S1144": {
            "level": "off"
        }
    },
    "editor.foldingImportsByDefault": false,
    "hediet.vscode-drawio.resizeImages": null,
    "editor.rulers": [
        { "column": 120, "color": "#8484845d" },
        { "column": 80, "color": "#ff000032" }
    ],
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.enabled": true,
    "java.format.settings.profile": "GoogleStyle",
    "javascript.inlayHints.parameterTypes.enabled": true,
    "typescript.inlayHints.parameterNames.enabled": "literals",
    "typescript.inlayHints.parameterTypes.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "literals",
    "java.inlayHints.parameterNames.enabled": "all",
    "editor.inlayHints.padding": true,
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "javascript.inlayHints.propertyDeclarationTypes.enabled": true,
    "javascript.inlayHints.variableTypes.enabled": true,
    "typescript.inlayHints.enumMemberValues.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
    "typescript.inlayHints.variableTypes.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,

    "java.completion.favoriteStaticMembers": [
        "org.junit.Assert.*",
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*"
    ],
    "cmake.configureOnOpen": true,
    "cmake.options.statusBarVisibility": "visible",
    "tabnine.experimentalAutoImports": true,
    "files.defaultLanguage": "${activeEditorLanguage}",
    "supermaven.enable": {
        "*": true
    },
    "workbench.iconTheme": "icons"
}