    ".tool-version": false,
    "yarn.lock": false,
    "**/types": false,
    "tsconfig.tsbuildinfo": false,
    ".npm-only-allow": false,
    ".turbo": false,
    ".gitattributes": false,
    ".eslintrc.cjs": false
